<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="DevicesTestPanel">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxBoxSizer" variable="BoxSizerTest" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT_DEV_TST_TTL" variable="StaticTextDevTestTitle" member="yes">
					<label>Audio devices test</label>
				</object>
				<flag>wxTOP|wxLEFT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizerTop" member="no">
					<object class="sizeritem">
						<object class="wxPanel" name="ID_PANEL_DEV_TEST" subclass="RimPanel" variable="PanelDevTest" member="yes">
							<object class="wxBoxSizer" variable="BoxSizerDevTest" member="no">
								<object class="sizeritem">
									<object class="wxButton" name="ID_BUTTON_TST_DEV_START" subclass="GButton" variable="ButtonDevTestStart" member="yes">
										<label>Start test</label>
										<size>100,22</size>
										<handler function="OnButtonDevTEstClick" entry="EVT_BUTTON" />
									</object>
									<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<border>5</border>
								</object>
								<object class="sizeritem">
									<object class="wxButton" name="ID_BUTTON_TST_DEV_STOP" subclass="GButton" variable="ButtonDevTestStop" member="yes">
										<label>Stop test</label>
										<size>100,22</size>
										<handler function="OnButtonDevTestStopClick" entry="EVT_BUTTON" />
									</object>
									<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<border>5</border>
								</object>
								<object class="spacer">
									<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<border>5</border>
									<option>1</option>
								</object>
							</object>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizerMiddle" member="no">
					<object class="sizeritem">
						<object class="wxPanel" name="ID_PANEL_PLAYBACK" subclass="RimPanel" variable="PanelPlayback" member="yes">
							<object class="wxBoxSizer" variable="BoxSizerPlayback" member="no" />
						</object>
						<flag>wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxPanel" name="ID_PANEL_RECORDING" subclass="RimPanel" variable="PanelRecording" member="yes">
							<object class="wxBoxSizer" variable="BoxSizerRecording" member="no" />
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizerBottom" member="no" />
				<flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</wxsmith>
